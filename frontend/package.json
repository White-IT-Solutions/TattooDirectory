{
  "name": "tattoo-artist-directory-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:docker": "next dev --hostname 0.0.0.0 --port 3000",
    "build": "next build",
    "build:analyze": "npm run build && npm run validate:build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "node src/__tests__/runIntegrationTests.js",
    "test:integration:coverage": "node src/__tests__/runIntegrationTests.js --coverage",
    "test:integration:watch": "node src/__tests__/runIntegrationTests.js --watch",
    "test:accessibility": "node src/__tests__/runIntegrationTests.js --accessibility-only",
    "test:visual": "jest --testPathPattern=VisualRegression.test.jsx",
    "test:cross-page": "jest --testPathPattern=CrossPageConsistency.test.jsx",
    "fix:build": "node scripts/fix-build-issues.js",
    "validate:build": "node scripts/validate-build.js",
    "deploy:prep": "node scripts/deploy-production.js"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.48.3",
    "@react-google-maps/api": "^2.20.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "next": "^15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "use-places-autocomplete": "^4.0.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "eslint": "8.57.1",
    "eslint-config-next": "15.5.2",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^4"
  }
}
