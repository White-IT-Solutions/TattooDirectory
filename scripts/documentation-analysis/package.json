{
  "name": "documentation-analysis",
  "version": "1.0.0",
  "description": "Documentation analysis and consolidation tools for the Tattoo Artist Directory MVP",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "node scripts/analyze-documentation.js",
    "consolidate": "node scripts/consolidate-documentation.js",
    "validate": "node scripts/validate-documentation.js",
    "gap-analysis": "node scripts/gap-analysis.js",
    "fix-outdated-content": "node scripts/fix-outdated-content.js",
    "consolidate-duplicates": "node scripts/consolidate-duplicates.js",
    "create-missing-docs": "node scripts/create-missing-docs.js",
    "generate-foundation": "node scripts/generate-foundation-docs.js",
    "validate-foundation": "node scripts/validate-foundation.js",
    "fix-consolidated-links": "node scripts/fix-consolidated-links.js",
    "pipeline": "node scripts/run-complete-pipeline.js",
    "pipeline:all": "node scripts/run-complete-pipeline.js --all",
    "pipeline:dry-run": "node scripts/run-complete-pipeline.js --dry-run",
