{
  "name": "tattoo-directory-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for Tattoo Artist Directory MVP",
  "private": true,
  "scripts": {
    "test": "npx mocha --timeout 30000 --recursive tests/**/*.test.js",
    "test:headless": "HEADLESS=true npm test",
    "test:visual": "npx mocha --timeout 30000 tests/visual/**/*.test.js",
    "test:workflows": "npx mocha --timeout 30000 tests/workflows/**/*.test.js",
    "test:integration": "npx mocha --timeout 30000 tests/integration/**/*.test.js",
    "test:watch": "npx mocha --timeout 30000 --watch tests/**/*.test.js",
    "test:debug": "DEBUG=true npm test",
    "setup": "node setup/install-dependencies.js",
    "clean": "node setup/cleanup.js"
  },
  "dependencies": {
    "puppeteer": "^21.5.0",
    "mocha": "^10.2.0",
    "chai": "^4.3.10",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "fs-extra": "^11.1.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0"
  }
}
